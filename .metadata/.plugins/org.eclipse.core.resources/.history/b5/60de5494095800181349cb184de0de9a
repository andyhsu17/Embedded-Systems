#include "msp.h"
#include "msp432p401r.h"
#include "msp432.h"

/**
 * main.c

 */
void main(void)
{

	WDT_A->CTL = WDT_A_CTL_PW | WDT_A_CTL_HOLD;		// stop watchdog timer
	volatile int i = 0;
	 P1->DIR |= BIT0;
	    P2->DIR |= BIT0;
	    P2->DIR |= BIT1;
	     P2->DIR |= BIT2;

	 P1->OUT &= ~BIT0;
	    P2->OUT &= ~BIT0;
	     P2->OUT &= ~BIT1;
	     P2->OUT &= ~BIT2;

while(1)
{
    P1->OUT |= BIT0;
    for(i = 0; i < 100000; i++);
    P1->OUT &= ~BIT0;
    for(i = 0; i < 100000; i++);

	P2->OUT |= BIT0;
	for(i = 0; i < 100000; i++);
    P2->OUT &= ~BIT0;
    for(i = 0; i < 100000; i++);

	P2->OUT |= BIT1;
    for(i = 0; i < 100000; i++);
    P1->OUT &= ~BIT1;
    for(i = 0; i < 100000; i++);

	P2->OUT |= BIT2;
    for(i = 0; i < 100000; i++);
    P2->OUT &= ~BIT2;
    for(i = 0; i < 100000; i++);


}
}
